.app-container {
  padding: 20px;
  /* Add other styles for the main app container */
}

.dark-mode-button {
  position: fixed;
  top: 20px;
  right: 20px;
  padding: 10px 20px;
  font-size: 16px;
  background-color: #007bff;
  color: #fff;
  border: none;
  border-radius: 5px;
  cursor: pointer;
}

.dark {
  background-color: #222;
  color: #fff;
}

.header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20px;
}


.button-group {
  display: flex;
  gap: 10px;
}

.logo-link {
  display: flex;
  justify-content: center;
  align-items: center;
  text-decoration: none;
}

.logo-image {
  width: 100px;
  height: 100px;
  object-fit: contain;
}

